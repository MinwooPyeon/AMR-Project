        "${workspaceFolder}/include/amr/ai",
        "${workspaceFolder}/include/amr/angle_controller.h",
        "${workspaceFolder}/include/amr/backend_ws_client.h",
        "${workspaceFolder}/include/amr/battery",
        "${workspaceFolder}/include/amr/camera",
        "${workspaceFolder}/include/amr/i2c",
